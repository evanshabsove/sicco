<!DOCTYPE html>
<html class="<%= css_slug %>">
  <head>
    <title>Sicco</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= stylesheet_link_tag 'application', 'https://fonts.googleapis.com/css?family=Lato', media: 'all'  %>
    <%= javascript_include_tag 'application' %>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
  </head>

  <body class="<%= css_slug %>">
  <nav class="navbar navbar-inverse <%= css_slug  %> navbar-fixed-top-<%= css_slug  %>">
    <div class="container-fluid">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar" aria-expanded="false" aria-controls="navbar">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="<%= root_url %>">Logo</a>
      </div>
      <div id="myNavbar" class="navbar-collapse collapse">
        <ul class="nav navbar-nav navbar-right links-<%= css_slug  %>">
          <li class="nav-item"><%= link_to "ABOUT", about_path %></li>
          <li class="nav-item"><%= link_to "AREA OF PRACTICE", area_of_practice_path %></li>
          <li class="nav-item"><%= link_to "TESTIMONIALS", testimonials_path %></li>
          <li class="nav-item"><%= link_to "LOCATION", location_path %></li>
        </ul>
      </div>
    </div>
  </nav>
  <!--end of nav bar -->
    <%= yield %>
  </body>
</html>
